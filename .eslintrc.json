{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "cypress/globals": true,
    "jest/globals": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly",
    "cy": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": [
    "codingitwrong",
    "prettier",
    "plugin:cypress/recommended"
  ],
  "plugins": ["prettier", "cypress", "jest", "react"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parser": "babel-eslint"
}
